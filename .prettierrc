  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "code": 80,
  "before": true,
  "after": true,
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrder": [
    "^@core/(.*)$",
    "^@src/(.*)$",
    "^@components/(.*)$",
    "^@pages/(.*)$",
    "^@planner/(.*)$",
    "^@api/(.*)$",
    "^[./]"
  ]
}
